class Bullet{
    // Instance variables
    field int x;
    field int y;
    field int size;

    constructor Bullet new (){
        let x = 10;
        let y = 10;
        let size = 10;
        do draw();
        return this;
    }

    method void draw() {
        do Screen.setColor(true);
        do Screen.drawCircle(x, y, 2);
        return;
    }
}